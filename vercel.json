{
   "version": 2,
   "builds": [
     { "src": "app.js", "use": "@vercel/node" }
   ],
   "routes": [
     { "src": "/anasayfa", "dest": "/views/anasayfa.ejs" },
     { "src": "/(.*)", "dest": "/app.js" }
   ],
   "rewrites": [
     { "source": "/views/anasayfa.ejs", "destination": "/anasayfa" }
   ]
}
